<body class="metro">
    @RenderBody()
<div style="position: fixed;
                  left: 0;
                  bottom: 0;
                  width: 100%;
                  background-color: #519fc9;
                  color: white;
                  text-align: center;">
    <div style="margin-bottom: 0.2%; margin-top: 0.2%;">
